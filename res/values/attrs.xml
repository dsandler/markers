<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PenToolButton">
        <attr name="strokeWidthMax" format="dimension" />
        <attr name="strokeWidthMin" format="dimension" />
        <attr name="interpolator" format="reference" />
    </declare-styleable>
    <declare-styleable name="SwatchButton">
        <attr name="color" format="color" />
    </declare-styleable>
    <declare-styleable name="PenTypeButton">
        <attr name="penType">
	        <enum name="whiteboard" value="0" />
	        <enum name="feltTip" value="1" />
	        <enum name="airbrush" value="2" />
        </attr>
    </declare-styleable>
</resources>